<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Praticien" table="praticien">
    <indexes>
      <index name="UNIQ_D9A27D32A8E0DF2" columns="Typ_code"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="praNom" type="string" column="Pra_Nom" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="praAdresse" type="string" column="Pra_Adresse" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="praCp" type="integer" column="Pra_CP" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="praVille" type="string" column="Pra_ville" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="coefnotoriete" type="integer" column="Coefnotoriete" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="typCode" target-entity="TypePraticien" fetch="LAZY">
      <join-columns>
        <join-column name="Typ_code" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
